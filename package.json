{
  "name": "orion",
  "version": "1.0.0-dev",
  "registry": "github",
  "description": "A visual framework for creating engaging and delightful web applications.",
  "keywords": ["html", "css", "css framework", "vsiual toolkit"],
  "author": "Kay Sackey",
  "contributors": ["Kekeli Sackey"],
  "license": "UNLICENSED",
  "private": true,
  "main": "src/index.js",
  "homepage": "https://github.com/9Cloud/Ceres",
  "repository": {
      "type": "git",
      "url": "https://github.com/9Cloud/Ceres.git"
  },
  "directories": {
      "lib": "orion",
      "examples": "html_prototypes"
  },
  "watch": {
    "compile:sass": {
      "patterns": [
        "scss"
      ],
      "extensions": "scss",
      "quiet": false
    },
    "dev_server": {
      "patterns": [
        "orion"
      ],
      "extensions": "js",
      "quiet": false
    },
    "proxy_server": {
      "patterns": [
        "server.js"
      ],
      "quiet": false
    },
    "browser_sync:dev": {
      "patterns": [
        "server_bs.js"
      ],
      "quiet": false
    }
  },
  "scripts": {
    "start": "npm run watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "bundle:deps": "jspm bundle orion/app - [orion/**/*] dist/dev/dependency-bundle.js --skip-source-maps --inject",
    "bundle:deps2": "jspm bundle prosemirror + markdown-it + react-router + react-virtualized dist/dev/dependency-bundle.js --skip-source-maps --inject",
    "bundle:app": "jspm bundle orion/app",
    "unbundle": "jspm unbundle",
    "compile:sass": "sassc -m scss/demo.scss scss/demo.css; sassc -m scss/main.scss scss/main.css;",
    "proxy_server": "NODE_ENV=development; babel-node server.js",
    "dev_server": "NODE_ENV=development; babel-node server_dev.js",
    "browser_sync:dev": "sleep 1; NODE_ENV=production; babel-node server_bs.js",
    "browser_sync:prod": "NODE_ENV=production; babel-node server_bs.js --bsOpen=external",
    "watch": "npm-watch",
    "dev": "webpack-dev-server --no-info"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "env": {
      "production": {
        "plugins": [
          "transform-react-constant-elements",
          "transform-react-inline-elements"
        ]
      }
    }
  },
  "jspm": {
    "main": "orion/index.js",
    "directories": {
      "lib": "orion",
      "examples": "html_prototypes"
    },
    "dependencies": {
      "tide": "github:tide@dev",
      "babel-polyfill": "npm:babel-polyfill@^6.9.1",
      "bluebird": "npm:bluebird@^3.4.1",
      "chance": "npm:chance@^1.0.4",
      "classnames": "npm:classnames@^2.2.5",
      "css": "github:systemjs/plugin-css@^0.1.26",
      "lodash": "npm:lodash@^4.5.0",
      "markdown-it": "npm:markdown-it@^7.0.0",
      "mobx": "npm:mobx@^2.3.7",
      "mobx-react": "npm:mobx-react@^3.5.1",
      "moment": "npm:moment@^2.14.1",
      "prosemirror": "npm:prosemirror@^0.9.1",
      "react": "npm:react@^0.14.8",
      "react-dom": "npm:react-dom@^0.14.8",
      "react-router": "npm:react-router@^2.6.1",
      "react-virtualized": "npm:react-virtualized@^7.18.1",
      "shallowequal": "npm:shallowequal@^0.2.2",
      "validator": "npm:validator@^5.5.0",
      "whatwg-fetch": "npm:whatwg-fetch@^0.11.0"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-preset-es2015": "npm:babel-preset-es2015@^6.3.13",
      "babel-preset-react": "npm:babel-preset-react@^6.3.13",
      "babel-preset-stage-0": "npm:babel-preset-stage-0@^6.5.0",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "capaj/systemjs-hot-reloader": "github:capaj/systemjs-hot-reloader@^0.5.5",
      "core-js": "npm:core-js@^1.1.4",
      "mobx-react-devtools": "npm:mobx-react-devtools@^4.2.4",
      "mobx-remotedev": "npm:mobx-remotedev@^0.1.3",
      "react-proptype-conditional-require": "npm:react-proptype-conditional-require@^1.0.2",
      "redbox-react": "npm:redbox-react@^1.3.0"

    }
  },
  "devDependencies": {
      "babel-core": "^6.14.0",
      "babel-eslint": "^6.1.2",
      "babel-loader": "^6.2.5",
      "babel-plugin-istanbul": "^2.0.1",
      "babel-plugin-react-transform": "^2.0.2",
      "babel-plugin-transform-decorators-legacy": "^1.3.4",
      "babel-plugin-transform-flow-strip-types": "^6.14.0",
      "babel-plugin-transform-runtime": "^6.15.0",
      "babel-preset-es2015": "^6.14.0",
      "babel-preset-es2015-loose": "^7.0.0",
      "babel-preset-es2015-native-modules": "^6.9.4",
      "babel-preset-react": "npm:babel-preset-react@^6.3.13",
      "babel-preset-stage-0": "npm:babel-preset-stage-0@^6.5.0",
      "babel-register": "^6.14.0",
      "classnames": "^2.2.5",
      "extract-text-webpack-plugin": "^2.0.0-beta.4",
      "file-loader": "^0.9.0",
      "html-webpack-plugin": "^2.22.0",
      "mobx-react-devtools": "^4.2.4",
      "mobx-remotedev": "^0.1.3",
      "redbox-react": "^1.3.0",
      "validator": "^5.5.0",
      "webpack": "^2.1.0-beta.22",
      "webpack-dev-server": "^2.1.0-beta.4"
    },
  "dependencies": {
    "tide": "git://github.com/KaySackey/tide.git#dev",
    "mobx": "^2.4.3",
    "mobx-react": "^3.5.5",
    "markdown-it": "^7.0.0",
    "prosemirror": "^0.9.1"
  }
}